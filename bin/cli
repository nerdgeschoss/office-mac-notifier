#!ruby -w

require_relative "../fritzbox.rb"

fritzbox = FritzBox.new(password: ENV["PW"])
fritzbox.login
body = { mac_addresses: fritzbox.devices.map(&:mac) }.to_json
HTTParty.post("https://office.nerdgeschoss.de/api/hooks/online_devices", body: body, headers: { "Content-Type" => "application/json" })
